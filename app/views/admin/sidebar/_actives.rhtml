<% if actives.blank? %>
  <div class="target">  Drag some plugins here to fill your sidebar!  </div>
<% else %>
 <%= render :partial => 'active', :collection => actives %>
<% end %>
<%= sortable_element "active", :url => {:action => 'set_active'}, :tag => 'div',
     :handle => 'handle', :containment => ['active','available_box'],
     :loading => "new Element.show('update_spinner')" %>
